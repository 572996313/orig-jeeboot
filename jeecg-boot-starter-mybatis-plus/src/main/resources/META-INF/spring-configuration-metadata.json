      "type": "org.jeecg.config.mybatis.JeecgMybatisPlusProperties$TenantProperties",
      "sourceType": "org.jeecg.config.mybatis.JeecgMybatisPlusProperties",
      "description": "多租户配置"
    },
    {
      "name": "jeecg.mybatis-plus.dynamic-table",
      "type": "org.jeecg.config.mybatis.JeecgMybatisPlusProperties$DynamicTableProperties",
      "sourceType": "org.jeecg.config.mybatis.JeecgMybatisPlusProperties",
      "description": "动态表名配置"
    },
    {
      "name": "jeecg.mybatis-plus.interceptor",
      "type": "org.jeecg.config.mybatis.JeecgMybatisPlusProperties$InterceptorProperties",
      "sourceType": "org.jeecg.config.mybatis.JeecgMybatisPlusProperties",
      "description": "SQL拦截器配置"
    },
    {
      "name": "jeecg.mybatis-plus.pagination",
      "type": "org.jeecg.config.mybatis.JeecgMybatisPlusProperties$PaginationProperties",
